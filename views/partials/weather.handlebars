<div class="weatherWidget">
    {{#each partials.weatherContext.locations}}
    <div class="location">
        <h3>
            {{name}}
        </h3>
        <a href="{{name}}">
            <img src="{{iconUrl}}" alt="{{weather}}"/>
            {{weather}}, {{temp}}
        </a>
    </div>
    {{/each}}
    <small>
        Source: <a href="http://wundergroud.com">Weather Underground</a>
    </small>
</div>